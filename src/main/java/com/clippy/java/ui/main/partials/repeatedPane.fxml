<?xml version="1.0" encoding="UTF-8"?>

<?import com.clippy.java.utils.utils.TitledPaneWithCtrl?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<TitledPaneWithCtrl fx:id="repeatedPane" animated="false" text="20 primeras letras" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.clippy.java.ui.main.partials.repeatedPaneController">
  <GridPane prefHeight="198.0" prefWidth="354.0">
    <columnConstraints>
      <ColumnConstraints/>
      <ColumnConstraints maxWidth="-Infinity" minWidth="-Infinity" prefWidth="95.0"/>
    </columnConstraints>
    <rowConstraints>
      <RowConstraints fillHeight="true" vgrow="ALWAYS"/>
    </rowConstraints>
    <TextArea fx:id="textArea" wrapText="true" text="hola mundo" GridPane.columnIndex="0"/>
    <VBox alignment="TOP_CENTER" spacing="10" GridPane.columnIndex="1">
      <HBox alignment="CENTER" spacing="15.0" VBox.vgrow="NEVER">
        <Button fx:id="upBtn" mnemonicParsing="false">
          <graphic>
            <ImageView fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
              <Image url="@up.png"/>
            </ImageView>
          </graphic>
        </Button>
        <Button fx:id="downBtn" mnemonicParsing="false">
          <graphic>
            <ImageView fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
              <Image url="@down.png"/>
            </ImageView>
          </graphic>
        </Button>
      </HBox>
      <ToggleButton fx:id="favoriteBtn" mnemonicParsing="false" text="Favorito" wrapText="true">
        <graphic>
          <ImageView fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
            <Image url="@star.png"/>
          </ImageView>
        </graphic>
      </ToggleButton>
      <Button fx:id="removeBtn" mnemonicParsing="false" prefWidth="79.0" text="Borrar" wrapText="true">
        <graphic>
          <ImageView fitHeight="16" fitWidth="16" pickOnBounds="true" preserveRatio="true">
            <Image url="@cross.png"/>
          </ImageView>
        </graphic>
      </Button>
    </VBox>
  </GridPane>
</TitledPaneWithCtrl>
